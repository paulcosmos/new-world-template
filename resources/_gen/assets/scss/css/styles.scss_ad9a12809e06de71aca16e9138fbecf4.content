        }

        a {
            color: $primary;
            transition: color 0.3s;

            &:hover{
            color: $secondary;
            }
        }

        a[href^="#"]{
            display: none!important;
        }
        .icon {
            color:inherit;
        }
    }
}

.social-links {
    display: grid;
    grid-gap: 1rem;
    justify-content: left;
    align-content: center;
    grid-auto-flow: column;
    margin-top: 2rem;
    margin-bottom: 2rem;

    @include until($navbar-breakpoint){
        margin-top: 2rem;
    }

    a {
        color: inherit;
        transition: color 0.3s;

        &:hover{
        color: inherit;
        }
    }

    a[href$="#"],
    {
        display: none!important;
    }
    
    .icon {
            color:inherit;
    }
}

.company {

    &-intro {

        .columns {

            &:not(:last-child){
                margin-bottom: $size-1!important;
                padding-bottom: $size-3;
                border-bottom: solid 2px $tertiary;
            }

            &:nth-of-type(odd){
                flex-direction: row-reverse;
            }
        }

        &-content {
        color: $secondary;

            h2 {
                color: inherit;
            }

        }

        .image {
            img {
                @include box-shadow-below;
            }
        }
    }

    &-services {

        &-overview {
            @include background-rings;
            background-position: bottom;
            background-color: $secondary;
            color: white;
            padding-bottom: 5rem;

            &-item {
                margin-bottom: 1rem;
                color: darken(white, 20%);

                h3 {
                    color: white;
                    font-size: $size-5;
                    margin-bottom: $size-7;
                }

                a {
                    display: inline-block;
                    color: inherit;
                    font-weight: 600;
                    margin-top: 1rem;

                    &:after {
                    @include animated-underline;
                    width:100%;
                    }

                    &:hover {
                        &:after {
                            width:100%;
                        }
                    }

                    &:hover {
                        color: $primary;
                    }
                }
            }
        }

        &-title {

        }

        &-item {

            &:not(:last-child){
                margin-bottom: $size-1!important;
                padding-bottom: $size-3;
                border-bottom: solid 2px $tertiary;
            }

            &:last-child{
                padding-bottom: $size-5;
            }

            // Flip the columns to make it pretty
            &:nth-of-type(odd){
                flex-direction: row-reverse;
            }

            hr {
                top: -10px;
                position: relative;
                display: inline-block;
                height: 3px;
                width: 60px;
                background-color: $primary;
                border: 0px !important;
            }

        }

        &-image {
            & > img {
                @include rounded-corners;
                @include box-shadow-below;
            }
        }
    }

    &-icon {
        color: $primary;
        width: 2rem;
        height: 2rem;
        font-size: 2rem;
    }
}

// Customer Reviews 
.review {

    &-grid,
    &-slider {
        .column {
            display: flex;
        }
    }

    &-slider {
        padding-bottom: $size-2;
    }

    &-card {
        @include box-shadow-stripe;
        background-color: white;
        border-radius: 0.5rem;
        display: flex;
        flex-grow: 1;
        border: solid 1px lighten(black, 95%);

            figure {
                text-align: left;
                margin-left: 0;
                margin-right: 0;

                figcaption {
                    font-size: $size-4;
                    font-weight: 600;
                    font-style: inherit;
                    text-align: inherit;
            }
        }
    }

    &-stars {
        color: $primary;
        margin: 0.5rem 0 1rem;
    }
}

// Image Gallery 
.gallery {
    display: grid;
    grid-template-columns: repeat(2,1fr);
    gap: 1rem;

    @include from($desktop) {
        grid-template-columns: repeat(3,1fr);
    }

    @include from($fullhd) {
        grid-template-columns: repeat(4,1fr);
    }

    a {
        flex-grow: 1;
        opacity: 1;
        transition: opacity 0.2s ease-in-out;
        &:hover {
            opacity: 0.5;
        }

        img {
            width: 100%;
            @include box-shadow-trello;

            &:hover {
                transition: box-shadow 0.3s ease-out;
                @include box-shadow-high;
            }
        }
    }

}

// Call to Action banner
.cta {

    &-banner {
        background-color: $primary;
        position: relative;
        margin-top: -5rem;
        padding: 2rem;
        font-weight: 600;
        @include rounded-corners;
        @include box-shadow-high;
        
        a {
            color:white;
            display: inline-block;

            &:after {
                @include animated-underline;
                background-color: white;
                width: 100%;
            }

            &:hover {
                // color: $primary;

                &:after {
                    width: 0%;
                }
            }
        }
    }
}

.accreditations {

    &-banner {
        display: grid;
        grid-gap: 1rem;
        grid-template-columns: repeat(auto-fill, minmax(128px, 0.5fr));
        // grid-auto-flow: column;
        padding-top: $size-4;
        padding-bottom: $size-2;

        @include until($fullhd){
        padding-left: 32px;
        padding-right: 32px;
        // grid-template-columns: repeat(3, 1fr);
        }

        @include from($desktop){
        grid-template-columns: none;
        grid-auto-flow: column;
        align-items: center;
        justify-items: center;
        // grid-template-columns: repeat(auto-fill, minmax(128px, 0.5fr));

        }

        img {
            @include box-shadow-below;
            display: block;
            }
        }

    &-grid {
        display: grid;
        grid-template-columns: repeat(auto-fill, minmax(128px, 0.5fr));
        gap: 1.75rem;

        @include until($desktop) {
            margin-top: 2rem;
        }

        img {
            @include box-shadow-below;
            &:nth-child(even) {
                @include from($navbar-breakpoint) {
                margin-top:2rem;
                }
                }
            }
        }
    }

// Why Us Banner
.why-us {

    &-banner {
        background-color: $tertiary;
        padding-top: 3rem;
        padding-bottom: 3rem;
        @include from($navbar-breakpoint) {
            padding: 3rem 32px;
        }
    }
}

// Quote Form Modal Pop-up
.modal-trigger {

    margin-left:inherit auto;
        @include from($navbar-breakpoint){
        margin-left:1rem;
    }

}

.quote-modal {

    .modal-card {

        // background-color: #fcfcfc;
        width: auto;

        &-head,
        &-body,
        &-foot {
            background-color: inherit;
            border: none;
        }
    }
}

// Contact Page
.contact {

    &-details {

        color: inherit;

        h2,h3,h4,h5,h6 {
            color: $accent;
        }

        address {
            .icon-text {
                margin-bottom: .5rem;
            }
        }

        .icon {

            &-text {
                display: block;

                .icon {
                    color: $primary;
                }
            }
        }

        a {
            color:inherit;

            &:hover {
                color:$primary;
            }
        }
    }

    &-form {
        padding: 0.75rem;
    }
}

.help {
    color: $grey-light;
}

// Map section
.map {
    margin-bottom: 3rem;
    width:100%;
}

.ol-viewport {
    min-height:500px;
    @include until($desktop){
        min-height:300px;
    }
}

// Site Footer 
.site-footer {
    background-color: $accent;
    color: white;
    @include background-paved;

    &-company-info {

        @include until($desktop) {
            margin-bottom:2rem;
        }
        h4 {
            margin-bottom: 1rem;
            font-size: $size-6;

            span {
                font-size: $size-4;
                font-weight: bold;
            }
        }
        h5{
            font-weight: normal;
            font-size: $size-7;
        }

        .button-primary {
            margin-top:2rem;
        }
    }

    &-links {
        a {
        font-size: $size-7;
        color: white;
        text-decoration: none;
        transition: color 0.3s;
        display: inline-block;

