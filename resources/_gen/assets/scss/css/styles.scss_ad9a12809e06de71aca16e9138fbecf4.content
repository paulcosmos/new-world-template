@import url('https://fonts.googleapis.com/css2?family=Nunito:wght@300;400;600;800&display=swap');
@import url('https://fonts.googleapis.com/css2?family=Poppins:wght@400;500&family=Roboto:wght@400;500&family=Work+Sans:wght@300;400;500&display=swap');
@import url('https://fonts.googleapis.com/css2?family=Roboto:wght@400;500&family=Work+Sans:wght@300;400;500&display=swap');

// Override Bulma variables here:
$primary: #3D8E07;
$secondary: #0A1A28;
$info: $secondary;
$light-grey: #f8f8f8;
$tertiary: #EFF2F5;
$family-sans-serif: "Nunito", sans-serif;
$family-headings: $family-sans-serif;
$text: $secondary;
$navbar-background-color: darken($secondary, 6%);
$navbar-height: 5rem;
$navbar-item-color: white;
$navbar-burger-color: white;
$navbar-item-hover-background-color: none;
$navbar-dropdown-arrow: $primary;
$navbar-dropdown-background-color: lighten($navbar-background-color, 5%);
$navbar-dropdown-item-hover-background-color: $navbar-dropdown-background-color;
$navbar-dropdown-item-hover-color: $primary;
$navbar-dropdown-border-top: 2px solid $primary;
$navbar-burger-color: $primary;
$navbar-item-hover-color: white;
$sl-overlay-background: $secondary;
$sl-navigation-color: white;
// $fullhd: 1440px;
$bg-overlay: rgba(0, 0, 0, 0.6);

@import "bulma.sass";
@import "simple-lightbox.scss";

$links: $primary;
$accent: $secondary;
$help-color: $warning;
$button-corner-radius: 4px;
$image-corner-radius: 0.5rem;;

// Settings for 'Dot's BG pattern
$bg-color: $tertiary;
$dot-color: darken($bg-color, 8%);
$dot-size: 2px;
$dot-space: 16px;
// Setting the the 'paved' BG pattern
$paving-size: 40px;
$paving-bg:$accent;
$paving: lighten($accent, 3%);

// Mixins 
@mixin section-item-title {
    color: inherit;
    font-size: 2rem;
    font-weight: 600;
    line-height: 1.125;
    word-break: break-word;
    margin-bottom:1rem;
}

@mixin x-item-title {
    color: inherit;
    font-weight: 600;
    font-size:1.25rem;
    margin-bottom: 0.75rem;
}

@mixin animated-underline {
    content: "";
    position: relative;
    bottom: 0;
    left: 50%;
    transform: translateX(-50%);
    display: block;
    width: 0%;
    height: 2px;
    background-color: $primary;
    transition: width 0.3s;
}

@mixin box-shadow-stripe {
    box-shadow: rgba(0, 0, 0, 0.1) 0px 20px 30px -10px;
}

@mixin box-shadow-trello {
    box-shadow: rgba(9, 30, 66, 0.25) 0px 4px 8px -2px, rgba(9, 30, 66, 0.08) 0px 0px 0px 1px;
}
@mixin box-shadow-offset-lines {
    box-shadow: rgba(0, 0, 0, 0.4) 0px 2px 4px, rgba(0, 0, 0, 0.3) 0px 7px 13px -3px, rgba(0, 0, 0, 0.2) 0px -3px 0px inset;
    box-shadow:  
    $tertiary 10px 10px 0px -2px, 
    $primary 10px 10px, 
    $tertiary 20px 20px 0px -2px, 
    $primary  20px 20px, 
    $tertiary 30px 30px 0px -2px, 
    $primary  30px 30px
}

@mixin box-shadow-3d {
    box-shadow: rgba(0, 0, 0, 0.4) 0px 2px 4px, rgba(0, 0, 0, 0.3) 0px 7px 13px -3px, rgba(0, 0, 0, 0.2) 0px -3px 0px inset;
}

@mixin box-shadow-below {
    box-shadow: rgba(0, 0, 0, 0.45) 0px 25px 20px -20px;
}

@mixin box-shadow-high {
    box-shadow: rgba(0, 0, 0, 0.2) 0px 20px 30px;
}

@mixin rounded-corners {
    border-radius: $image-corner-radius;
}

@mixin background-image {
    background-size: cover;
    background-repeat: no-repeat;
    background-position: center;
}

@mixin background-rings {
    background-color: $secondary;
    background-image:  repeating-radial-gradient( circle at 0 0, transparent 0, $secondary 11px ), repeating-linear-gradient( $secondary, lighten($secondary, 5%) );
}

@mixin background-dots {
	background:
		linear-gradient(90deg, $bg-color ($dot-space - $dot-size), transparent 1%) center,
		linear-gradient($bg-color ($dot-space - $dot-size), transparent 1%) center,
		$dot-color;
	background-size: $dot-space $dot-space;
}

@mixin background-dots-offset {
    background-color: $accent;
    background-image: url("data:image/svg+xml,%3Csvg width='20' height='20' viewBox='0 0 20 20' xmlns='http://www.w3.org/2000/svg'%3E%3Cg fill='ffffff' fill-opacity='0.2' fill-rule='evenodd'%3E%3Ccircle cx='3' cy='3' r='3'/%3E%3Ccircle cx='13' cy='13' r='3'/%3E%3C/g%3E%3C/svg%3E"); 
}

@mixin background-paved {
    background:
        linear-gradient(135deg,#0000 18.75%,$paving-bg 0 31.25%,#0000 0),
        repeating-linear-gradient(45deg,$paving-bg -6.25% 6.25%,$paving 0 18.75%);
    background-size: $paving-size $paving-size; 
}

html {
    font-size:19px;
}

html,
body {
    width: auto!important; 
    overflow-x: hidden!important
}


// Global Helpers

.nwdm-link {
    color: inherit;
    text-decoration: none;

    &:hover {
        color:$primary;
    }
}

.has-background {

    &-image {
        @include background-image;
    }

    &-rings {
        @include background-rings;
    }

    &-dots {
        @include background-dots;
    }
    
    &-dots-offset {
        @include background-dots-offset;
    }
    &-paved {
        @include background-paved;
    }
}

.has-rounded-corners {
    border-radius: $image-corner-radius;
}

.image {
    
    &.has-rounded-corners {
        > img {
            border-radius: $image-corner-radius;
            // border-radius: 10% 25%;
            // border-right-width: 3rem;
            border-color: $primary;
        }
    }
}

// Buttons 
.button {

    &-primary > a,
    &-primary > button {
        background-color: $primary;
        border-color: transparent;
        color: white;
        border-width: 1px;
        // font-size: 1rem;
        font-size: inherit;
        cursor: pointer;
        justify-content: center;
        padding-bottom: calc(0.5em - 1px);
        padding-left: 1em;
        padding-right: 1em;
        padding-top: calc(0.5em - 1px);
        text-align: center;
        white-space: nowrap;
        -webkit-appearance: none;
        align-items: center;
        border: 1px solid transparent;
        border-radius: $button-corner-radius;
        box-shadow: none;
        display: inline-flex;

        &:hover {
            background-color: lighten($primary, 10%);
            border-color: transparent;

            i {
                margin-left: 1.2rem;
            }
        }

        i {
            transition: all 0.3s;
            margin-left: 0.75rem;
        }
    }

    &-fullwidth > a {
        display: flex;
        width: 100%;
    }
}

// Typography

h1,h2,h3,h4,h5,h6 {

    font-family: $family-headings;
}

h2 {
    @include section-item-title;
}

h3 {
    @include x-item-title;

}

address {
    font-style: normal;
    display: inline;
}

.content {

    &.media {
        margin-bottom: 0;
    }

    h1, h2, h3, h4, h5, h6,
    p, ul, li {
        color: inherit;
    }
    
    ul {
        position: relative;
        margin-left: 0;
    
        li {
            list-style: none;   
            position: relative;
            padding-left: 1.5rem;
            font-weight: 600;

            &:before {
                content: " ";
                position: absolute;
                left: 0;
                top: 0.5rem;
                bottom: 0;
                width: 0.5rem;
                height: 0.5rem;
                line-height: 2rem;
                text-align: center;
                border-radius: 50%;
                background-color: $primary;
            } 
        }
    }
}

.has-accent {

    white-space: normal;

    strong,
    span {
        color: $primary;
        font-weight: inherit;
    }
}

// Top Banner
.top-banner {
    background-color: $accent;
    color: white;
    font-size: $size-7;
    font-weight: medium;
    padding-top: $size-7;
    padding-bottom: $size-7;
    position: relative;
    // z-index: 99;
    .icon {
        color: $primary;
    }

    a {
        color:inherit;
    }
}

// Navigation Menu
.navbar {
    // z-index: 99;

    &-brand {
        
        .navbar-item {
            @include until($navbar-breakpoint) {
                padding-left: 0;
            }
        }
    }

    &-title {
        text-align: left;
        font-size: $size-4;
        font-weight: 600;

        &:hover {
            transition: color 0.3s;
            color: $primary;
        }

        span {
        display: block;
        font-size: $size-7;
        color: $tertiary;
        }

    }
    
    &-item {
        color: white;
        transition: all 0.3s;
        text-align: center;
        position: relative;

    }

    &-menu {
        transition: all 0.3s;

        &.is-active {
            @include until($navbar-breakpoint) {
                width:100%;
                min-height:100vh;
                display:flex;
                position: fixed;
                top: 0;
                left: 0;
                // z-index: 99;
                background-color: rgba(0,0,0,0.94);
            }
        }
    }

    &-dropdown {
        font-size: 1rem;
    }

    &-end {
        @include until($navbar-breakpoint){
            min-height: 100%;
            display: flex;
            flex-direction: column;
            flex-grow: 1;
            justify-content: center;
            align-items: center;
        }
    }

    & > .navbar-item {
        @include until($navbar-breakpoint){
            flex: 1;
            display: flex;
            flex-direction: column;
            justify-content: center;
            text-align: center;
        }
      }

    &-burger {
        text-align: center;
        position: relative;
        &:hover {
            color: white;
            background-color: $primary;
        }

        &.is-active {
            z-index: 99;
            color: white;
            &:before {
                content: "Close";
                text-transform: uppercase;
                font-weight: bold;
                display: inline-block;
                position: relative;
                height: 5rem;
                line-height: 5rem;
                top: 50%;
                left: -50%;
                transform: translateX(0%) translateY(-50%);
            }
            &:hover {
                background-color: initial;
                color: $primary;
            }
        }

        span {
            height: 2px;
        }
    }
}

.site-nav-links {
    
    text-align: center;

    & > a {
        &:after {
            @include animated-underline;
            display: block;
            position: absolute;
            bottom: 25%;
            width:0%;
            z-index: 99;

            @include until($navbar-breakpoint) {
                bottom:0;
            }
        }

        &:hover {
            &:after {
                width:100%;
            }
        }
    }
}

// Inherit font colour from parents Bulma modifier classes 

.pre-heading,
.pre-heading > span {
    text-transform: uppercase;
    font-size: 0.75rem;
    font-weight: bold;
    margin-bottom: $size-4;
    display: block;
    color: $primary;
}

// Page Content
.hero { 
    
    @include background-dots;
    color: inherit;
    text-align: left; 
    color: white;
    color: inherit;
    text-align: inherit;

    @include from($navbar-breakpoint){
        padding-top: 3rem;
        padding-bottom: 3rem;
    }

    &-index {
    color: inherit;
    text-align: inherit;
    }

    &-services {
    color: inherit;
    text-align: inherit;
    }

    &-gallery {
    color: inherit;
    text-align: inherit;
    }

    &-reviews {
    color: inherit;
    text-align: inherit;
    }

    &-contact {
    color: inherit;
    text-align: inherit;
    }

    &-fail {
    color: inherit;
    text-align: inherit;

    .has-accent {
        span,
        strong {
            color:$danger;
            }
        }
    }

    &-success {
    color: inherit;
    text-align: inherit;
    }

    &-socials {
        display: grid;
        grid-gap: 1rem;
        justify-content: center;
        align-content: center;
        grid-auto-flow: column;
        margin-top: 2rem;

        @include until($navbar-breakpoint){
